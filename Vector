class Vector
{
  float _x;
  float _y;
  
  Vector(float x, float y)
  {
    _x = x;
    _y = y;
  }
  
  Vector vectorAdd(Vector another)
  {
    Vector add;
    add = clone();
    add._x += another._x;
    add._y += another._y;
    
    return add;
  }
  
  Vector vectorDiff(Vector another)
  {
   Vector diff;
   diff = clone();
   diff._x -= another._x;
   diff._y -= another._y;
   return diff;
  }
  
  
  Vector clone()
  {
    Vector clone;
    clone = new Vector(this._x, this._y);
    return clone;
  }
